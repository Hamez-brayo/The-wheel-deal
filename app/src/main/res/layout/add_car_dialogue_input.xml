<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="351dp"
    app:cardCornerRadius="25dp"
    android:backgroundTint="@color/background"
    android:layout_margin="0dp"
    android:layout_height="639dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="639dp"
        android:padding="16dp">

        <ImageView
            android:id="@+id/imageView61"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentDescription="@string/addbackgound"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/addrecbackground"
            tools:ignore="ImageContrastCheck" />

        <ImageView
            android:id="@+id/imageView62"
            android:layout_width="157dp"
            android:layout_height="45dp"
            android:layout_marginBottom="8dp"
            android:contentDescription="@string/AddButton"
            app:layout_constraintBottom_toBottomOf="@+id/imageView61"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/addbutton"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:text="@string/add"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/imageView62"
            app:layout_constraintEnd_toEndOf="@+id/imageView62"
            app:layout_constraintStart_toStartOf="@+id/imageView62"
            app:layout_constraintTop_toTopOf="@+id/imageView62"
            app:layout_constraintVertical_bias="0.523"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="@string/type"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintEnd_toStartOf="@+id/textInputLayout"
            app:layout_constraintHorizontal_bias="0.285"
            app:layout_constraintStart_toStartOf="@+id/imageView61"
            app:layout_constraintTop_toBottomOf="@+id/textView24" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="@string/model"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView22"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/textView22"
            app:layout_constraintTop_toBottomOf="@+id/textView22" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="@string/regno_string"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView25"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView25"
            app:layout_constraintTop_toBottomOf="@+id/textView25" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="@string/image_string"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView26"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView26"
            app:layout_constraintTop_toBottomOf="@+id/textView26" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="@string/descriptiontb_string"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView27"
            app:layout_constraintHorizontal_bias="0.25"
            app:layout_constraintStart_toStartOf="@+id/textView27"
            app:layout_constraintTop_toBottomOf="@+id/textView27" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_bold"
            android:text="@string/add_car"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView61"
            app:layout_constraintStart_toStartOf="@+id/imageView61"
            app:layout_constraintTop_toTopOf="@+id/imageView61" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="171dp"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"


            android:layout_marginEnd="20dp"
            android:padding="0dp"
            app:boxStrokeColor="#E2E2E2"
            app:endIconMode="clear_text"
            app:layout_constraintBottom_toBottomOf="@+id/textView22"
            app:layout_constraintEnd_toEndOf="@+id/imageView61"
            app:layout_constraintTop_toTopOf="@+id/textView22"
            app:layout_constraintVertical_bias="0.571">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="172dp"
                android:layout_height="55dp"
                android:background="#CDFFFFFF"
                android:elegantTextHeight="true"
                android:fontFamily="sans-serif"
                android:textColor="@color/my_apptheme"
                android:textSize="14sp"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/modelInputLayout"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="171dp"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"


            android:padding="0dp"
            app:boxStrokeColor="#E2E2E2"
            app:endIconMode="clear_text"
            app:layout_constraintBottom_toBottomOf="@+id/textView25"
            app:layout_constraintEnd_toEndOf="@+id/textInputLayout"
            app:layout_constraintStart_toStartOf="@+id/textInputLayout"
            app:layout_constraintTop_toTopOf="@+id/textView25"
            app:layout_constraintVertical_bias="0.666">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="172dp"
                android:layout_height="55dp"
                android:background="#CDFFFFFF"
                android:elegantTextHeight="true"
                android:fontFamily="sans-serif"
                android:textColor="@color/my_apptheme"
                android:textColorHint="#8D6E63"
                android:textSize="12sp"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/RegnoInputLayout"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="171dp"
            android:layout_height="55dp"
            android:layout_marginHorizontal="16dp"


            android:padding="0dp"
            app:boxStrokeColor="#E2E2E2"
            app:endIconMode="clear_text"
            app:layout_constraintBottom_toBottomOf="@+id/textView26"
            app:layout_constraintEnd_toEndOf="@+id/modelInputLayout"
            app:layout_constraintStart_toStartOf="@+id/modelInputLayout"
            app:layout_constraintTop_toTopOf="@+id/textView26"
            app:layout_constraintVertical_bias="0.619">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="172dp"
                android:layout_height="55dp"
                android:background="#CDFFFFFF"
                android:elegantTextHeight="true"
                android:fontFamily="sans-serif"
                android:textColor="@color/my_apptheme"
                android:textColorHint="#8D6E63"
                android:textSize="12sp"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        </com.google.android.material.textfield.TextInputLayout>

        <ImageView
            android:id="@+id/imageView64"
            android:layout_width="87dp"
            android:layout_height="82dp"
            android:contentDescription="@string/select_image_string"
            app:layout_constraintBottom_toBottomOf="@+id/textView27"
            app:layout_constraintStart_toStartOf="@+id/RegnoInputLayout"
            app:layout_constraintTop_toTopOf="@+id/textView27"
            app:layout_constraintVertical_bias="0.327"
            app:srcCompat="@drawable/rectangle" />

        <ImageView
            android:id="@+id/imageView65"
            android:layout_width="40dp"
            android:layout_height="37dp"
            android:contentDescription="@string/add_image_string"
            app:layout_constraintBottom_toBottomOf="@+id/imageView64"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64"

            app:srcCompat="@drawable/ic_baseline_add_circle_outline_24" />

        <ImageView
            android:id="@+id/imageView67"
            android:layout_width="172dp"
            android:layout_height="150dp"
            android:contentDescription="@string/descriptions_string"
            app:layout_constraintBottom_toBottomOf="@+id/textView28"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/textView28"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/rectangle" />

        <EditText
            android:id="@+id/editTextTextMultiLine"
            android:layout_width="172dp"
            android:layout_height="150dp"
            android:autofillHints=""
            android:elegantTextHeight="true"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:textColor="@color/my_apptheme"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView67"
            app:layout_constraintEnd_toEndOf="@+id/imageView67"
            app:layout_constraintStart_toStartOf="@+id/imageView67"
            app:layout_constraintTop_toTopOf="@+id/imageView67"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>

